  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "chokidar": "^1.5.1",
    "jest": "^22.4.0",
    "should": "^8.3.0",
    "supertest": "^1.2.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.3",
    "tsconfig-paths": "^3.3.2",
    "typescript": "^2.8.3"
  },
  "license": "MIT"
}
